<section id="log_in">
<div class="section-form">
<div class="col-md-6">
  <h3>Sign in</h3>
  <form role="form">
    <div class="form-group">
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="form-group">
        <%=f.label :email %></br>
        <%= f.email_field :email, autofocus: true, :class=> "form-control", :placeholder => "Enter email" %>
      </div>
      <div class="form-group">
        <%=f.label :password %></br>
        <%= f.password_field :email, autocomplete: "off", :class=> "form-control", :placeholder => "Password" %>
      </div>
      <div><%= f.submit "Sign in", :class=>"btn btn-default" %></div>
      <div class="form-group">
        <%= render "devise/shared/links" %>
      </div>
      <%end%>
    </form>
    </div>
</div>
</section>